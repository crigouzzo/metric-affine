 RowBox[{
  RowBox[{"sol", "=", 
   RowBox[{"Simplify", "[", 
    RowBox[{
     RowBox[{"Solve", "[", 
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{
          RowBox[{"2", 
           SubscriptBox["C", "2"], "*", "That"}], "+", 
          RowBox[{
           SubscriptBox["C", "3"], "*", "T"}], "+", 
          RowBox[{
